.content
  .page-header
    if user && user.is_admin
      a(href='/create-event/').pull-right.btn Create an event
    h1 All Events

  each e in events
    h2
      a(href='/events/' + e.event.id)= e.event.title
      small= e.org.name
    p= e.event.start_time + ' - ' + e.event.end_time
    p= e.event.description
